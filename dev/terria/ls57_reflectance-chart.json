"template": "<table>  <tr><td><b>Time</b></td><td>{{time}}</td></tr>  <tr><td><b>blue</b></td><td>{{bands.blue}}</td></tr>  <tr><td><b>green</b></td><td>{{bands.green}}</td></tr>  <tr><td><b>red</b></td><td>{{bands.red}}</td></tr>  <tr><td><b>nir</b></td><td>{{bands.nir}}</td></tr>  <tr><td><b>swir2</b></td><td>{{bands.swir1}}</td></tr>  <tr><td><b>swir2</b></td><td>{{bands.swir2}}</td></tr>  <tr><td><b>NDVI (derived)</b></td><td>{{band_derived.ndvi}}</td></tr>  <tr><td><b>NDWI (derived)</b></td><td>{{band_derived.ndwi}}</td></tr></table><p><chart id='{{lat}}{{lon}}{{time}}' title='NBART at {{lat}},{{lon}}' column-units='Wavelength (nm), Reflectance x 10k' preview-x-label='NBART Reflectance'>nm,NBART Reflectance\n478,{{bands.blue}}\n560,{{bands.green}}\n662,{{bands.red}}\n835,{{bands.nir}}\n1648,{{bands.swir1}}\n2205,{{bands.swir2}}</chart></p><p><b>Imagery available for dates:</b>{{#data_available_for_dates}}<br/>{{.}}{{/data_available_for_dates}}</p>"
